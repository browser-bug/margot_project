{
	"application": "bar",
	"version": "4.0",
	"blocks":
	[
		{
			"name": "foo",

			"monitors":
			[
				{
					"name": "my_custom_monitor",
					"type": "float",
					"log": [ "average" ],
					"constructor": [ 10 ]
				},
				{
					"name": "my_time_monitor",
					"type": "time",
					"log": [ "average" ],
					"constructor": [ "margot::TimeUnit::MICROSECONDS", 1 ]
				}
			],


			"knobs":
			[
				{
					"name": "knob1",
					"type": "int",
					"values": [ 1, 3, 10, 20 ]
				},
				{
					"name": "knob2",
					"type": "float",
					"range": [ 1.0, 2.0, 0.2 ]
				}
			],

			"metrics":
			[
				{
					"name": "metric1",
					"type": "int",
					"distribution": "yes",
					"prediction_plugin": "hth",
					"prediction_parameters":
					{
						"param1": "fsdf",
						"param2": "sdfsf"
					},
					"observed_by": "my_custom_monitor"
				}
			],

			"feature_distance":"euclidean",
			"features":
			[
				{
					"name": "bla_bla",
					"type": "int",
					"comparison": "le"
				},
				{
					"name": "feature2",
					"type": "float",
					"comparison": "-"
				}
			],

			"agora":
			{
				"borker_url": "127.0.0.1:1883",
				"broker_username": "margot",
				"broker_password": "margot",
				"broker_qos": 2,
				"broker_ca": "",
				"client_cert": "",
				"client_key": "",
				"clustering_plugin": "kmeans",
				"clustering_parameters":
				{
					"algorithm": "kmeans",
					"number_centroids": 5
				},
				"doe_plugin": "banane_volanti",
				"doe_parameters":
				{
					"constraint": "knob1 + knob2 < 40",
					"custom_param1": "bla"
				}
			},

			"extra-functional_requirements":
			[
				{
					"name":"default",
					"maximize":
					{
						"geometric_mean":
						{
							"metric1": 4.0,
							"knob1": 1.0
						}
					},
					"subject_to":
					[
						{
							"subject":"metric1",
							"comparison": "lt",
							"value": 5.0,
							"confidence": 1.0,
							"reactive_inertia": 2
						}
					]
				}
			]
		}
	]
}