{
	"application": "bar",
	"version": "4.0",
	"blocks":
	[
		{
			"name": "foo",

			"monitors":
			[
				{
					"name": "quality_monitor",
					"type": "float",
					"log": [ "average" ]
				},
				{
					"name": "throughput_monitor",
					"type": "throughput",
					"log": [ "average" ]
				}
			],


			"knobs":
			[
				{
					"name": "threads",
					"type": "int",
					"range": [ 1, 32, 1 ]
				},
				{
					"name": "algorithm",
					"type": "string",
					"values": [ "one", "two", "three" ]
				}
			],

			"metrics":
			[
				{
					"name": "throughput",
					"type": "float",
					"distribution": "yes",
					"observed_by": "throughput_monitor",
					"reactive_inertia": 5
				},
				{
					"name": "quality",
					"type": "float"
				}
			],

			"feature_distance":"euclidean",
			"features":
			[
				{
					"name": "resolution",
					"type": "int",
					"comparison": "le"
				}
			],

			"extra-functional_requirements":
			[
				{
					"name":"default",
					"maximize":
					{
						"geometric_mean":
						{
							"quality": 1
						}
					},
					"subject_to":
					[
						{
							"subject":"throughput",
							"comparison": "ge",
							"value": 25.0,
							"confidence": 3
						}
					]
				}
			]
		}
	]
}